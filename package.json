{
  "name": "react-material-kit",
  "version": "1.0.0",
  "description": "基于 React 16.0 优雅的 Material Design UI 组件库",
  "main": "dist/index.js",
  "module": "dist/es2015/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --config scripts/webpack.dev.js",
    "build": "npm run compile:cjs && npm run compile:ems5 && npm run compile:ems2015",
    "compile:cjs": "tsc -p ./scripts/tsconfig.cjs.json",
    "compile:ems5": "tsc -p ./scripts/tsconfig.ems5.json",
    "compile:umd": "webpack --config ./scripts/webpack.umd.js",
    "build:site": "webpack --config scripts/webpack.site.js",
    "clean": "rm -rf dist site"
  },
  "author": "stbui",
  "license": "MIT",
  "tags": [
    "stbui",
    "react",
    "react-stbui"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "stbui",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "conventional-changelog-cli": "^2.0.1",
    "css-loader": "^1.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
