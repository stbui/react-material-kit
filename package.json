  },
  "dependencies": {
    "classnames": "^2.2.6",
    "marked": "^0.5.2",
    "prismjs": "^1.15.0",
    "react": "^16.6.3",
    "react-docgen": "^3.0.0",
    "react-dom": "^16.6.3",
    "react-markdown": "^4.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
